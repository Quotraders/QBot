name: Market Maker Positioning
on:
  schedule:
    # 24/7 MARKET MAKER POSITIONING (CRITICAL FOR LIQUIDITY ANALYSIS)
    # Asian Session (18:00-23:59 CT) - Monitor Asian MM activity
    - cron: '0 1,3,5 * * *'              # Every 2 hours Asian session
    # European Session (02:00-05:00 CT) - Monitor European MM flows
    - cron: '30 8,9,10,11 * * *'         # Every hour European session
    # US Session (08:30-16:00 CT) - Maximum MM positioning frequency
    - cron: '0,30 14,15,16,17,18,19,20,21 * * 1-5'  # Every 30 minutes US session
    # After Hours MM Analysis
    - cron: '45 6,7,12,13,22,23 * * *'   # Extended hours MM tracking
    # Total: ~29 runs/day = 203 runs/week for continuous MM positioning
  workflow_dispatch:


permissions:
  contents: write

jobs:
  mm_positioning:
    runs-on: ubuntu-latest
    steps:
      - uses: actions/checkout@v4
      - uses: actions/setup-python@v4
        with:
          python-version: '3.9'
      - run: pip install yfinance pandas numpy scipy requests beautifulsoup4 ta pytrends fredapi
      - run: python Intelligence/scripts/mm_positioning.py
      
      - name: "ðŸ”— Integrate with BotCore Decision Engine"
        run: |
          echo "ðŸ”— Converting Market Maker Positioning to BotCore format..."
          
          # Run data integration script for MM positioning
          python Intelligence/scripts/workflow_data_integration.py \
            --workflow-type "mm_positioning" \
            --data-path "Intelligence/data/" \
            --output-path "Intelligence/data/integrated/mm_positioning_analysis.json"
          
          echo "âœ… BotCore MM positioning integration complete"
      
      - run: |
          git config user.name "GitHub Actions"
          git config user.email "actions@github.com"
          git add Intelligence/data/
          git add Intelligence/data/integrated/
          git diff --quiet || git commit -m "ðŸ“Š MM Positioning: BotCore-integrated analysis $(date -u +%Y%m%d_%H%M%S)"
          git push
