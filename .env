
# GitHub Cloud Learning (FREE!)
CLOUD_PROVIDER=github
RL_ENABLED=1
GITHUB_CLOUD_LEARNING=1

# Bot Configuration for Development/Testing
BOT_MODE=paper
TOPSTEPX_API_BASE=https://api.topstepx.com
TOPSTEPX_RTC_BASE=https://rtc.topstepx.com
RTC_USER_HUB=https://rtc.topstepx.com/hubs/user
RTC_MARKET_HUB=https://rtc.topstepx.com/hubs/market
ASPNETCORE_URLS=http://localhost:5050

# Demo/Development Credentials (for dashboard testing)
# In production, these should be set via secure environment or secrets
TOPSTEPX_USERNAME=demo@example.com
TOPSTEPX_API_KEY=demo-key-123
TOPSTEPX_ACCOUNT_ID=12345
TOPSTEPX_JWT=demo-jwt-token-for-dashboard-testing
AUTH_ALLOW=false

# Offline Development Mode
BOT_OFFLINE_MODE=true
DEMO_MODE=true
SKIP_EXTERNAL_CONNECTIONS=true
ENABLE_DEMO_DATA=true

# Health Monitoring
ENABLE_HEALTH_MONITORING=true
HEALTH_CHECK_INTERVAL=60

# Dashboard Features
ENABLE_DASHBOARD=true
ENABLE_DETAILED_LOGGING=true

# Risk Management
DAILY_LOSS_CAP_R=3.0
PER_TRADE_R=1.0
MAX_CONCURRENT=1

# Session Configuration  
SKIP_TIME_WINDOWS=1
SKIP_ATTEMPT_CAPS=1

